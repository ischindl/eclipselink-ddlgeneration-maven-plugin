<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
         name="${project.artifactId}">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>

    <publishDate format="yyyy-MM-dd"/>

    <version position="right"/>

    <body>
        <menu name="Documentation">
            <item name="Home" href="/index.html"/>
        </menu>

        <menu name="Modules" ref="modules" inherit="none"/>
        <menu name="Reports" ref="reports" inherit="none"/>

        <menu name="Links" inherit="none" inheritAsRef="true">
            <item name="Bitbucket Homepage" href="https://bitbucket.org/boraxhacker" />
        </menu>

    </body>
</project>